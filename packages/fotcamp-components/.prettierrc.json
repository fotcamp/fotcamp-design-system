{
  "printWidth": 100,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": false,
  "quoteProps": "as-needed",
  "trailingComma": "none",
  "bracketSpacing": true,
  "arrowParens": "avoid",
  "proseWrap": "preserve",
  "endOfLine": "auto",
  "htmlWhitespaceSensitivity": "css",
  "embeddedLanguageFormatting": "off",
  "importOrder": [
    "^@types/(.*)$",
    "^@components/(.*)$",
    "^@hooks/(.*)$",
    "^@router/(.*)$",
    "^@shared/(.*)$",
    "^@utils/(.*)$",
    "^@style/(.*)$",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
